{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Consolidated Member Application and Support Plan Schema",
  "properties": {
    "generalInfo": {
      "type": "object",
      "properties": {
        "prospective_member_name": { "type": "string" },
        "main_contact": { "type": "string" },
        "relationship": { "type": "string" },
        "date_of_birth": { "type": "string" },
        "primary_phone": { "type": "string" },
        "mailing_address": {
          "type": "object",
          "properties": {
            "street": { "type": "string" },
            "city": { "type": "string" },
            "state": { "type": "string" },
            "zip_code": { "type": "string" }
          },
          "description": "If different from above"
        },
        "email": { "type": "string" },
        "primary_diagnosis": { "type": "string" },
        "secondary_diagnosis": { "type": "string" },
        "school_name": {
          "type": "string",
          "description": "If still a student"
        },
        "referral_source": {
          "type": "string",
          "description": "How did you hear about us?"
        },
        "sex": {
          "type": "string",
          "enum": ["Male", "Female"],
          "description": "Sex of the applicant."
        }
      }
    },
    "programsServices": {
      "type": "object",
      "properties": {
        "campus_interest": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["Phoenix Campus", "Maricopa Campus", "Casa Grande Campus", "Avondale Campus"]
          },
          "description": "Checkboxes on the form; select all that apply."
        },
        "funding_source": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["Division of Developmental Disabilities", "Private Pay (PP)", "Other"]
          },
          "description": "Checkboxes on the form; select all that apply."
        },
        "support_contact": {
          "type": "object",
          "properties": {
            "name": { "type": "string" },
            "phone": { "type": "string" },
            "email": { "type": "string" }
          },
          "description": "DDD Support Coordinator/Service Contact"
        },
        "selected_programs": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "Adult Day Training/Habilitation/Activities",
              "Work Center Vocational Training",
              "Community-Based Enclave",
              "Individual Supported Employment",
              "Youth/School-Age/Transition Programs",
              "Weekend Program",
              "Transportation"
            ]
          },
          "description": "What Programs/Services Interest You; checkboxes on the form, select all that apply."
        },
        "transportation": {
          "type": "object",
          "description": "Transportation details (properties cut off in source schema)"
        }
      }
    },
    "guardianFamilyInformation": {
      "type": "object",
      "properties": {
        "guardian": {
          "type": "object",
          "properties": {
            "guardian_type": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": ["Biological/Adoptive Parent", "Legal Guardian", "Conservator", "Other (Specify)"]
              },
              "description": "Checkboxes on the form; select one."
            },
            "name": { "type": "string" },
            "relationship": { "type": "string" },
            "address": {
              "type": "object",
              "properties": {
                "street": { "type": "string" },
                "city": { "type": "string" },
                "state": { "type": "string" },
                "zip_code": { "type": "string" }
              }
            },
            "email": { "type": "string" },
            "phone": { "type": "string" }
          }
        },
        "co_guardian": {
          "type": "object",
          "properties": {
            "guardian_type": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": ["Same as Guardian", "Co-Guardian/Parent"]
              },
              "description": "Checkboxes on the form; select one."
            },
            "name": { "type": "string" },
            "relationship": { "type": "string" },
            "address": {
              "type": "object",
              "properties": {
                "street": { "type": "string" },
                "city": { "type": "string" },
                "state": { "type": "string" },
                "zip_code": { "type": "string" }
              }
            }
          }
        },
        "emergency_contact_secondary": {
          "type": "object",
          "properties": {
            "name": { "type": "string" },
            "relationship": { "type": "string" },
            "home": { "type": "string" },
            "work_cell": { "type": "string" }
          },
          "description": "Emergency Contact, Secondary"
        },
        "residential_manager": {
          "type": "object",
          "properties": {
            "name": { "type": "string" },
            "residence_phone": { "type": "string" },
            "manager_work_cell": { "type": "string" }
          },
          "description": "Residential Manager (if applicable)"
        }
      }
    },
    "demographics": {
      "type": "object",
      "properties": {
        "general": {
          "type": "object",
          "properties": {
            "member_id_code": { "type": "string", "description": "Office use only" },
            "authorization_start_date": { "type": "string", "description": "Office use only" },
            "date_of_birth": { "type": "string" },
            "address": {
              "type": "object",
              "properties": {
                "street": { "type": "string" },
                "city": { "type": "string" },
                "state": { "type": "string" },
                "zip_code": { "type": "string" },
                "phone": { "type": "string" }
              }
            },
            "race": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": ["Caucasian/White", "Black/African American", "Native Hawaiian/Other Pacific Islander", "American Indian/Alaskan Native", "Asian", "Other Multi-Racial"]
              },
              "description": "Checkboxes on the form; select all that apply."
            },
            "ethnicity": {
              "type": "string",
              "enum": ["Hispanic", "Non-Hispanic"],
              "description": "Checkboxes on the form"
            },
            "language": {
              "type": "string"
            }
          }
        },
        "militaryStatus": {
          "type": "object",
          "properties": {
            "military_status": {
              "type": "string",
              "enum": ["Veteran", "Active Duty", "None"],
              "description": "Checkboxes on the form"
            },
            "served_in_combat": { "type": "boolean" },
            "branch": { "type": "string" },
            "start_date": { "type": "string" },
            "end_date": { "type": "string" },
            "discharge_type": { "type": "string" }
          }
        },
        "educationWork": {
          "type": "object",
          "properties": {
            "highest_grade_completed": { "type": "integer" },
            "currently_in_school": { "type": "boolean" },
            "employed": { "type": "boolean" },
            "employment_type": {
              "type": "string",
              "enum": ["Full Time", "Part Time", "Seasonal"]
            },
            "job_title": { "type": "string" },
            "employer": { "type": "string" }
          }
        },
        "disability_type": {
          "type": "object",
          "properties": {
            "disabilities_receiving_services": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": ["Autism", "Cerebral Palsy", "Epilepsy", "Cognitive Impairment", "Developmental Disability (DD)", "DD Re: Work", "DD Re: Transportation", "No Disability", "Other"]
              },
              "description": "Checkboxes on the form; select all that apply."
            },
            "primary_disability_description": {
              "type": "string"
            }
          }
        },
        "resources": {
          "type": "object",
          "properties": {
            "benefits": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": ["Social Security", "SSI", "SSDI", "AHCCS", "Other:", "TANF", "Food Stamps", "General Assistance", "Veteran's Compensation"]
              },
              "description": "PLEASE CHECK ALL BENEFITS PARTICIPANT RECEIVES; Checkboxes on the form, select all that apply."
            },
            "other_benefits_specify": { "type": "string" }
          }
        },
        "household": {
          "type": "object",
          "properties": {
            "residential_setting": {
              "type": "string",
              "enum": ["Family (Of Origin) Home", "Group Supported Living (Group Home)", "Adult Developmental Home (ADH)", "Individually Designed Living Arrangement (IDLA)"],
              "description": "TYPE OF RESIDENTIAL SETTING; Checkboxes on the form."
            },
            "household_size": {
              "type": "integer",
              "description": "TOTAL NUMBER OF INDIVIDUALS LIVING IN THE HOUSEHOLD INCLUDING PARTICIPANT"
            }
          }
        }
      }
    },
    "healthAndMedical": {
      "type": "object",
      "properties": {
        "additional_diagnosis_medical_needs": {
          "type": "string",
          "description": "Additional Relevant Diagnosis/Medical Needs"
        },
        "insurance": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "insurance_company": { "type": "string" },
              "policy_number": { "type": "string" }
            }
          },
          "description": "Supports multiple insurance entries"
        },
        "height": { "type": "string" },
        "weight": { "type": "string" },
        "chronic_conditions": {
          "type": "string",
          "description": "Chronic or Reoccurring Medical Conditions"
        },
        "special_health_instructions": {
          "type": "string",
          "description": "Special Health-Related Instructions"
        }
      }
    },
    "seizureInformation": {
      "type": "object",
      "properties": {
        "history_of_seizures": {
          "type": "boolean",
          "description": "Does the applicant have any history of seizures?; Checkboxes on the form."
        },
        "seizure_activity_past_5_years": {
          "type": "boolean",
          "description": "Has the applicant had any seizure activity in the past 5 years?; Checkboxes on the form."
        },
        "last_seizure_date": {
          "type": "string",
          "description": "When (month/year) of the last seizure?"
        },
        "seizure_control_with_medication": {
          "type": "string",
          "enum": ["Completely controlled", "Partially controlled", "Not controlled"],
          "description": "Are the seizures currently being controlled with medication?; Checkboxes on the form."
        },
        "seizure_frequency": {
          "type": "string",
          "description": "How frequently the applicant has seizures (times per day, week, month, or year)?"
        },
        "seizure_duration_before_intervention": {
          "type": "string",
          "description": "How long can a seizure last before medical intervention becomes necessary?"
        },
        "antecedents_to_seizure": {
          "type": "string",
          "description": "Describe any antecedents to a seizure:"
        },
        "post_seizure_behavior_changes": {
          "type": "string",
          "description": "Is there any noticeable changes in behavior after a seizure (e.g. groggy, confused, irritable, etc)?"
        },
        "typical_seizure_presentation": {
          "type": "string",
          "description": "How does a seizure typically present itself? Please describe the physical characteristics, and the typical duration:"
        },
        "emergency_medical_interventions": {
          "type": "string",
          "description": "Describe medical interventions that are appropriate and available in an emergency (nerve stimulator, medication, length of seizure before calling paramedics, etc.)."
        },
        "other_seizure_info": {
          "type": "string",
          "description": "Please discuss any other information relating to seizures that is important for the individual's safety?"
        }
      }
    },
    "mobility": {
      "type": "object",
      "properties": {
        "mobility_status": {
          "type": "string",
          "enum": ["Fully Ambulatory", "Independent with Wheelchair", "Needs Full Assistance"],
          "description": "Checkboxes on the form; select one."
        },
        "walks_with_assistance": {
          "type": "boolean"
        },
        "walks_assistance_type": {
          "type": "string"
        },
        "wheelchair_usage": {
          "type": "boolean"
        },
        "wheelchair_type": {
          "type": "string"
        },
        "wheelchair_assistance": {
          "type": "string"
        },
        "climbing_stairs": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["Independent", "Independent with Railings", "Requires Supervision", "Requires Assistance"]
          }
        },
        "transferring": {
          "type": "string"
        }
      }
    },
    "feeding": {
      "type": "object",
      "properties": {
        "eating_ability": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["Independent (All Foods)", "Soft Foods Only", "With Adaptive Equipment", "Requires Assistance and/or Supervision"]
          },
          "description": "Checkboxes on the form; select all that apply."
        },
        "adaptive_equipment_description": { "type": "string" },
        "assistance_supervision_description": { "type": "string" }
      }
    },
    "personalCareMotorCoordination": {
      "type": "object",
      "properties": {
        "fine_motor_impairments": {
          "type": "string",
          "description": "Include options for assistance (hand over hand, modified utensils, etc.)"
        },
        "toileting_menstrual_care": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["Independent", "Occasional or Regular Reminders", "Requires Assistance"]
          }
        },
        "toileting_assistance_description": { "type": "string" },
        "other_healthcare_needs": {
          "type": "string",
          "description": "OTHER ADDITIONAL HEALTH CARE NEEDS/ROUTINES (i.e. tooth brushing, flossing, nail care, etc.): "
        },
        "grooming_personal_care": {
          "type": "object",
          "properties": {
            "special_instructions_bathing": { "type": "boolean", "description": "SPECIAL INSTRUCTIONS FOR BATHING:" },
            "methods": { "type": "string" },
            "products": { "type": "string" },
            "supervision": { "type": "string" },
            "gender_preference": { "type": "string", "enum": ["Male", "Female", "No Preference"] }
          }
        }
      }
    },
    "prescription_medication": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "medication_name": { "type": "string" },
          "time_administered": { "type": "string" },
          "dosage": { "type": "string" }
        }
      }
    },
    "communicationAndSocialLife": {
      "type": "object",
      "properties": {
        "clearly_communicates": {
          "type": "boolean",
          "description": "Does the applicant clearly communicate their wants and needs?; Checkboxes on the form."
        },
        "speech_description": {
          "type": "string",
          "enum": ["Easily understood by everyone", "Understood by those familiar with them", "Limited verbal ability", "Non-verbal"],
          "description": "Which most accurately describes the applicant's speech?; Checkboxes on the form."
        },
        "easiest_expression_methods": {
          "type": "object",
          "properties": {
            "methods": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": ["Speech", "Sign language", "Gesturing", "Communication", "Other"]
              },
              "description": "Checkboxes on the form; select all that apply."
            },
            "other_description": {
              "type": "string"
            }
          },
          "description": "What form of communication is the easiest way for the applicant to express themselves to others?"
        },
        "easiest_understanding_methods": {
          "type": "object",
          "properties": {
            "methods": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": ["Speech", "Sign language", "Gesturing", "Communication", "Other"]
              },
              "description": "Checkboxes on the form; select all that apply."
            },
            "other_description": {
              "type": "string"
            }
          },
          "description": "What form of communication makes it easiest for the applicant to understand others?"
        },
        "communication_considerations": {
          "type": "string",
          "description": "Are there any considerations that we should make when communicating with the applicant?"
        },
        "currently_receives_therapies": {
          "type": "string",
          "description": "Does the applicant currently receive any therapies? If so, please briefly describe their current goals so we can help provide consistent standards for the applicant's communication while they're in program."
        },
        "expressive_communication_needs": {
          "type": "object",
          "properties": {
            "express_wants_needs": {
              "type": "boolean",
              "description": "Can resident express wants and needs:"
            },
            "communication_type": {
              "type": "string",
              "description": "Type of Communication (i.e. verbal, sign, gestures, augmentative devices, etc.):"
            },
            "communication_steps": {
              "type": "string",
              "description": "If NO, steps to take to meet resident's needs:"
            }
          }
        }
      }
    },
    "behaviorAndPersonality": {
      "type": "object",
      "properties": {
        "age_appropriate_social_skills": {
          "type": "boolean",
          "description": "Does the applicant exhibit age appropriate social skills?"
        },
        "non_age_appropriate_behavior_description": {
          "type": "string",
          "description": "If no, describe the behavior that is not age appropriate:"
        },
        "behavioral_concerns": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "concern_type": {
                "type": "string",
                "enum": [
                  "Hurtful to self (suicidal)",
                  "Hurtful to others",
                  "Destructive to property",
                  "Repetitive behavior",
                  "Withdrawn or inattentive behavior",
                  "Running away",
                  "Verbal aggression",
                  "Physical aggression",
                  "Pica",
                  "Fall frequently",
                  "Lacks stranger danger skills",
                  "Risk of exploitation",
                  "Other:"
                ]
              },
              "description": {
                "type": "string",
                "description": "Description of the behavior"
              },
              "frequency": {
                "type": "string",
                "description": "Frequency (times per day, week, month, or year)"
              }
            },
            "description": "Table in form each row represents one entry"
          },
          "description": "Please indicate which of the following have ever been concerns for the applicant. Check all that apply."
        },
        "context_of_behavior": {
          "type": "string",
          "description": "If yes to any of the above, please describe the context of the behavior, including when the last instance was, possible causes of the behavior, and effective techniques to resolve the situation. Feel free to include additional pieces of paper to give the most detailed information possible."
        },
        "behavior_management_plan": {
          "type": "string",
          "description": "If any of these behaviors have caused a concern fot the safety of the applicant or other people, is anything currently being done to manage the behavior? This might include a formally implemented Behavior Treatment Plan, an approach to handling stress, therapy, or avoiding particular triggers for the behavior."
        },
        "warning_signs_before_behavior": {
          "type": "string",
          "description": "Does the applicant have any behaviors that come before the harmful behavior? What warning signs are exhibited by the applicant that would help us deescalate the situation before it becomes unsafe?"
        },
        "social_interaction_preference": {
          "type": "string",
          "enum": [
            "The applicant enjoys interacting with others",
            "The applicant prefers to spend time by themselves",
            "The applicant is withdrawn at first, but outgoing once they are comfortable in a situation"
          ],
          "description": "Check the option that you feel is most accurate:"
        },
        "preferred_environment": {
          "type": "string",
          "enum": ["Calm, low stimulus and relaxing", "Active, stimulating and engaging"],
          "description": "What sort of environment does the applicant prefer most of the time?"
        },
        "sensory_sensitivities": {
          "type": "string",
          "description": "Does the applicant have any sensitivity to light, sound, touch, or other sensory perceptions that we might consider in order to create a positive and comfortable environment for them?"
        },
        "enjoyable_activities": {
          "type": "string",
          "description": "What are some activities that the applicant enjoys, or that they might enjoy if given the opportunity? This might include participation in arts programs (like painting, photography, or theatre), regular physical exercise, volunteering in the community, or something that they've expressed interest in the past. Please be as specific as possible."
        },
        "behavioral_support": {
          "type": "object",
          "properties": {
            "btp": {
              "type": "boolean",
              "description": "Is a Behavior Treatment Plan (BTP) in place?"
            },
            "btp_target_behaviors": {
              "type": "string",
              "description": "IF YES, TARGET BEHAVIORS:"
            },
            "other_interfering_behaviors": {
              "type": "string",
              "description": "Other interfering behaviors not requiring BTP:"
            }
          }
        }
      }
    },
    "Learning_and_Personal_Growth": {
      "type": "object",
      "properties": {
        "time_comprehension_week": {
          "type": "boolean",
          "description": "Can the applicant identify the current day of the week?"
        },
        "time_comprehension_year": {
          "type": "boolean",
          "description": "Can the applicant identify the current day of the year?"
        },
        "clock_comprehension_digital": {
          "type": "boolean",
          "description": "Can the applicant tell the time on a digital clock?"
        },
        "clock_comprehension_analog": {
          "type": "boolean",
          "description": "Can the applicant tell the time on a analog clock?"
        },
        "number_comprehension": {
          "type": "integer",
          "description": "What is the highest number the applicant can count to without assistance?"
        },
        "math_comprehension": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "Completes basic addition and subtraction problems",
              "Completes basic multiplication and division problems",
              "Math skills are equivalent to a freshman in high school",
              "Math skills are equivalent to a high school graduate",
              "Math skills are at a post high school level"
            ]
          },
          "description": "What is the applicant's current math comprehension? Check all that apply:"
        },
        "writing_comprehension": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "Writes a short essay or paragraph independently",
              "Writes short sentences",
              "Writes their name with assistance",
              "Writes their name independently",
              "Does not write"
            ]
          },
          "description": "What is the applicant's current writing comprehension? Check all that apply:"
        },
        "reading_comprehension": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "Identifies letters and sounds",
              "Identifies letters, sounds, and identifies symbols (stop signs, restroom signs, etc.)",
              "Simple reading (around the level of a 3rd grader)",
              "Average reading (at least middle school or junior high level)",
              "Advanced reading (post high school level)"
            ]
          },
          "description": "What is the applicant's current reading comprehension?"
        },
        "academicAndLifeSkills": {
          "type": "object",
          "properties": {
            "skills_to_improve": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "Reading",
                  "Writing",
                  "Math",
                  "Money management",
                  "Communicating with others more clearly",
                  "Communicating with others in a more positive way",
                  "Cooking",
                  "Cleaning (house work)",
                  "Safety skills",
                  "Other"
                ]
              },
              "description": "Select all that apply."
            },
            "other_description": {
              "type": "string",
              "description": "If 'Other' is selected, specify the skill."
            },
            "description": "Which academic and life skills would be most beneficial for the applicant to spend time improving? Check all that apply:"
          }
        },
        "teaching_techniques": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["Hand over hand instruction", "Step by step spoken instructions", "Step by step written instructions", "Instructor modeling", "Peer modeling", "Self-Correction/Redirection"]
          },
          "description": "Which teaching techniques are the most effective when teaching the applicant new information? Select all that apply."
        },
        "instructional_steps_followed": {
          "type": "integer",
          "description": "How many instructional steps can the applicant follow at a time?"
        },
        "other_teaching_techniques": {
          "type": "string",
          "description": "Please describe any other teaching techniques that might be effective:"
        },
        "program_goals": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "Have something to do during the day",
              "Enjoy being in a fun, active, positive environment",
              "Make new friends",
              "Increase self-esteem",
              "Learn new daily living skills",
              "Learn new academic skills",
              "Learn new work skills",
              "Earn a paycheck",
              "Work in one of our community enclaves",
              "Develop the skills to find a job independently",
              "Develop the skills to live independently"
            ]
          },
          "description": "What would the applicant most like to get out of their time at The Opportunity Tree? Some of these goals may be achieved within a few weeks of being in the program, while others may take several years. Consider what some of those short term goals might be, as well as some long term goals (in the next 5-10 years); select all that apply."
        }
      }
    },
    "medicalHistory": {
      "type": "object",
      "properties": {
        "individual_name": {
